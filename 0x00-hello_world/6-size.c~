#include <stdio.h>
/**
 * main - Entry point
 *
 * Return: Always 0 (Success)
 */
int main(void)
{
        printf("Size of a char:", sizeof(char)\n)
	printf("Size of an int:", sizeof(int)\n)
	printf("Size of a long int:", sizeof(long)\n)
	printf("Size of a long long int:",sizeof(long)\n)
        printf("Size of a float:", sizeof(float)\n)
        return (0);
}
