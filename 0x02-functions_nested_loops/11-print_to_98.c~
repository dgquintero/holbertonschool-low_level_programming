#include "holberton.h"
#include <stdio.h>
/**
 * print_to_98 - check the code for Holberton School students.
 *
 * @n : parameter to check
 *
 * Return: print all natural numbers.
 */

void print_to_98(int n)
{
	int d;

	if (n > 98 && n != 98)
	{
		for (d = n; d >= 98; d--)
		{
			printf("%d", d);
			if (d != 98)
				printf(", ");
		}
	}
	else if (n < 98)
	{
		for (d = n; d <= 98; d++)
		{
			printf("%d", d);
			if (d != 98)
				printf(", ");
		}
	}
	else
		printf("%d", d);
	printf("\n");
}
